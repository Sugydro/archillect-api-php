{
  "version": 2,
  "name": "api-archillect",
  "alias": "api-archillect",
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.3.0"
    }
  },
  "routes": [ 
    { "src": "/([^/]+)", "dest": "/index.php?method=$1" },
    { "src": "/([^/]+)/([^/]+)", "dest": "/index.php?method=$1&int=$2" }
  ]
}
